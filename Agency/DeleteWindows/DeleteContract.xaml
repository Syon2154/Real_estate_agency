<Window x:Class="Agency.DeleteContract"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agency"
        mc:Ignorable="d"
        Title="Видалення договору" Height="200" Width="520" Background="#FF938989" FontSize="14" FontFamily="Arial" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" Closed="Window_Closed">
    <Grid>
        <Label x:Name="label1" Content="Введіть індекс договору, який необхідно видалити :" HorizontalAlignment="Left" Margin="20,15,0,0" VerticalAlignment="Top" Width="365" Height="30"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="30" Margin="390,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="105"/>
        <Button x:Name="del" Content="Видалити договір" HorizontalAlignment="Left" Margin="20,65,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="Del_Click"/>
        <Button x:Name="clear" Content="Очистити поле" HorizontalAlignment="Left" Margin="20,110,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="Clear_Click"/>
    </Grid>
</Window>
